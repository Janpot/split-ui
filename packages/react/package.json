{
  "name": "@split-ui/react",
  "version": "0.0.3",
  "description": "A React component library for flexible panel layouts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "commonjs",
  "style": "./dist/styles.css",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
  "keywords": [
    "react",
    "flex",
    "panels",
    "layout",
    "components"
  ],
  "author": "Jan Potoms",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Janpot/split-ui.git",
    "directory": "packages/react"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3"
  }
}

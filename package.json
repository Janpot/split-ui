{
  "name": "split-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --color --parallel dev",
    "build": "pnpm -r build",
    "start": "pnpm -F docs start",
    "test": "pnpm -r --color test",
    "test:ui": "pnpm -r test:ui",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typescript": "tsc --build",
    "prettier": "prettier --check \"**/*.{js,mjs,cjs,ts,jsx,tsx,json,md,mdx,css}\"",
    "format": "pnpm prettier --write",
    "release": "pnpm -F \"./packages/**\" build && pnpm -r publish"
  },
  "devDependencies": {
    "@arethetypeswrong/core": "^0.18.2",
    "@babel/generator": "^7.28.0",
    "@babel/types": "^7.28.2",
    "@eslint/js": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/browser-playwright": "^4.0.13",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "eslint": "^9.33.0",
    "eslint-config-next": "^16.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "pkg-pr-new": "^0.0.62",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "publint": "^0.3.12",
    "tsdown": "^0.18.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "unplugin-unused": "^0.5.1",
    "vitest": "^4.0.13",
    "vitest-browser-react": "^2.0.2",
    "vitest-fail-on-console": "^0.10.1"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@latest"
    }
  },
  "packageManager": "pnpm@10.27.0"
}
